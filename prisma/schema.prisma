generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id         String @id @default(uuid()) // coluna id tipo string, primary key e com auto incremento
  name       String
  github_id  Int
  avatar_url String
  login      String

  // nome da tabela
  @@map("users")
}
